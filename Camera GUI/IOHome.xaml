<Page x:Class="Camera_GUI.IOHome"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Camera_GUI"
      mc:Ignorable="d" 
      d:DesignHeight="500" d:DesignWidth="800"
      Title="IOGUI - Home">

    <Grid Margin="0" Background="#FFDAE3F3">
        <Grid.Resources>
            <Style TargetType="{x:Type Button}">
                <Setter Property="Padding" Value="5,2"/>
            </Style>
        </Grid.Resources>
        
        <Button Click = "Button_Click" Height="30" Width="60" Margin="0,0,276,235" HorizontalAlignment="Right" VerticalAlignment="Bottom">Level (Y)</Button>
        <Button Click = "Button_Click" Height="30" Width="60" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center">Level (X)</Button>
    </Grid>
</Page>
