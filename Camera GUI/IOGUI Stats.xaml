        <Rectangle Width="50" Height="50" Margin="0" Canvas.Left="235" Canvas.Top="141" Fill="Coral" Tag="platform"/>
        <Rectangle Width="50" Height="50" Margin="0" Canvas.Left="458" Canvas.Top="261" Fill="Plum" Tag="thing"/>
